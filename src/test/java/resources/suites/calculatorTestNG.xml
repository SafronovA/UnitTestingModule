<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Calculator" verbose="1" parallel="classes" thread-count="4" >

    <test name="testNgTests" enabled="false">
        <classes>
            <class name="com.epam.tat.module4.testNgTests.CosTest"/>
            <class name="com.epam.tat.module4.testNgTests.CtgTest"/>
            <class name="com.epam.tat.module4.testNgTests.DivTest"/>
            <class name="com.epam.tat.module4.testNgTests.IsNegativeTest"/>
            <class name="com.epam.tat.module4.testNgTests.IsPositiveTest"/>
            <class name="com.epam.tat.module4.testNgTests.PowTest"/>
            <class name="com.epam.tat.module4.testNgTests.SinTest"/>
            <class name="com.epam.tat.module4.testNgTests.SqrtTest"/>
            <class name="com.epam.tat.module4.testNgTests.TgTest"/>
        </classes>
    </test>

    <test name="packageRun" enabled="false">
        <packages>
            <package name="com.epam.tat.module4.testNgTests"/>
        </packages>
    </test>

    <test name="jUnitTests" junit="true" enabled="true">
        <packages>
            <package name="com.epam.tat.module4.junitTests"/>
        </packages>
    </test>

</suite>